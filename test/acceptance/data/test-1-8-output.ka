%agent: Ligand(r,loc~cytosol~membrane,loc_index_1~0~1)
%agent: Receptor-cytosol(dl,state~inactive~active,loc~cytosol~membrane,loc_index_1~0~1)
%agent: Receptor-membrane(dl,ligand,loc~cytosol~membrane,loc_index_1~0~1)


Ligand(r),Receptor-membrane(dl!1,ligand),Receptor-cytosol(dl!1,state~inactive) -> Ligand(r!2),Receptor-membrane(dl!1,ligand!2),Receptor-cytosol(dl!1,state~active) @ 'link rate'
Ligand(r!2),Receptor-membrane(dl!1,ligand!2),Receptor-cytosol(dl!1,state~active) -> Ligand(r),Receptor-membrane(dl!1,ligand),Receptor-cytosol(dl!1,state~inactive) @ 'unlink rate'

%init: 250 Receptor-cytosol(dl!1,loc~cytosol,loc_index_1~0),Receptor-membrane(dl!1,loc~membrane,loc_index_1~0)
%init: 250 Receptor-cytosol(dl!1,loc~cytosol,loc_index_1~0),Receptor-membrane(dl!1,loc~membrane,loc_index_1~1)
%init: 250 Receptor-cytosol(dl!1,loc~cytosol,loc_index_1~1),Receptor-membrane(dl!1,loc~membrane,loc_index_1~0)
%init: 250 Receptor-cytosol(dl!1,loc~cytosol,loc_index_1~1),Receptor-membrane(dl!1,loc~membrane,loc_index_1~1)
%init: 50 Ligand(r,loc~membrane,loc_index_1~0)
%init: 50 Ligand(r,loc~membrane,loc_index_1~1)

%var: 'link rate' 10.0
%var: 'unlink rate' 10.0
%var: 'inactive' Receptor-cytosol(state~inactive)
%var: 'active' Receptor-cytosol(state~active)
%plot: 'inactive'
%plot: 'active'

