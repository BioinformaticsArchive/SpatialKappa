%agent: A(s)
%agent: B(s)

%compartment: cytosol [2]
%compartment: membrane [2]

%channel: out cytosol [x] <-> membrane [x]
%channel: in membrane [x] <-> cytosol [x]


%init: 1000 A:cytosol(s!1:out),B:membrane(s!1)

%obs: 'all monomer A' A(s) # Expect none
%obs: 'all monomer B' B(s) # Expect none

%obs: 'all dimer' A(s!_)
%obs: 'dimer AB' A:cytosol(s!1:out),B:membrane(s!1) 
%obs: 'dimer BA' A:membrane(s!1:in),B:cytosol(s!1) # Expect none

%obs: 'same voxel dimer AB' cytosol A(s!1),B(s!1) # Expect none
%obs: 'same voxel dimer BA' membrane A(s!1),B(s!1) # Expect none

